	<objectives>
		<objective id="11" rank="Secondary" name="Welcome!" event="" score="0">
			<narrative />
			<clues>
				<clue id="28" name="Welcome to the Flowershop" type="Audio" source="stories/SBTF/Welcome (1).mp3" score="0" weight="1">
